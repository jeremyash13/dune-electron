{
  "productName": "Dune",
  "name": "dune",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@openfonts/jost_latin": "0.0.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "electron-is-dev": "^1.2.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "music-metadata": "^6.4.0",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-sound": "^1.2.0",
    "react-ticker": "^1.2.2",
    "react-transition-group": "^4.4.1"
  },
  "homepage": "./",
  "main": "electron/electron-starter.js",
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "dune-electron",
      "owner": "jeremyash"
    }
  },
  "scripts": {
    "watch:css": "postcss src/tailwind.css -o src/App.css -w",
    "react-start": "react-scripts start",
    "electron": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build": "react-scripts build",
    "build-electron": "robocopy electron build/electron /s",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build:css": "postcss src/tailwind.css -o src/App.css",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron/electron-starter.js --publish never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "electron": "^15.5.5",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.2.1",
    "electron-reload": "^1.5.0",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "^1.4.6"
  }
}
